{
    "framework": "vite",
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "outputDirectory": "dist",
    "functions": {
        "api/webdav-proxy.ts": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/webdav-proxy",
            "destination": "/api/webdav-proxy.ts"
        },
        {
            "source": "/((?!api).*)",
            "destination": "/index.html"
        }
    ]
}